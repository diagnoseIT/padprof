      <xs:attribute type="xs:int" name="s_c"/>
      <xs:attribute type="xs:int" name="f_r"/>
      <xs:attribute type="xs:int" name="f_w"/>
      <xs:attribute type="xs:int" name="s_o"/>
      <xs:attribute type="xs:int" name="f_cw"/>
      <xs:attribute type="xs:int" name="f_o"/>
      <xs:attribute type="xs:int" name="j"/>
